{
  "private": true,
  "name": "dumi-template",
  "version": "1.0.0",
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && dumi dev",
    "docs:build": "SET NODE_OPTIONS=--openssl-legacy-provider && dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && father-build",
    "deploy": "SET NODE_OPTIONS=--openssl-legacy-provider && npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0000ofl/dumi-ui.git",
    "branch": "main"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "@ant-design/icons": "^5.3.0",
    "@types/react": "^18.2.49",
    "@types/react-dom": "^18.2.18",
    "ali-react-table": "^2.6.1",
    "antd": "^5.13.3",
    "date-arithmetic": "^4.1.0",
    "luxon": "^3.4.4",
    "moment": "^2.30.1",
    "react-big-calendar": "^1.8.7",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
